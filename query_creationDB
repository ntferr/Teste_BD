CREATE DATABASE "Teste"
    WITH 
    OWNER = postgres
    ENCODING = 'UTF8'
    LC_COLLATE = 'C'
    LC_CTYPE = 'C'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1;
    
CREATE TABLE person
(
    person_id SERIAL PRIMARY KEY,
    person_name VARCHAR(50),
    age INTEGER,
    skills VARCHAR(130)
)
